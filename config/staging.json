{
    "auth": {
        "domain": ".delphishq.com"
    },
    "aws": {
        "region": "us-west-2",
        "useCredentials": false,
        "isFargate": true
    },
    "env": "staging",
    "db": {
        "region": "us-west-2",
        "tables_config": {
            "discussions": {
                "table_name": "staging.Discussions.v1"
            },
            "participants": {
                "table_name": "staging.Participants.v1"
            },
            "post_bookmarks": {
                "table_name": "staging.PostBookmarks.v1"
            },
            "posts": {
                "table_name": "staging.Posts.v1"
            },
            "users": {
                "table_name": "staging.Users.v1"
            },
            "user_profiles": {
                "table_name": "staging.UserProfiles.v1"
            },
            "viewers": {
                "table_name": "staging.Viewers.v1"
            }
        }
    },
    "twitter": {
        "callback": "http://staging.delphishq.com/twitter/callback",
        "redirect": "https://app-staging.delphishq.com/"
    }
}