{
    "auth": {
        "domain": ".delphishq.com"
    },
    "aws": {
        "region": "us-west-2",
        "useCredentials": false
    },
    "ably": {
        "enabled": true
    },
    "env": "local_use_aws",
    "sqldb": {
        "host": "127.0.0.1",
        "port": 3333,
        "db_name": "chatham_staging"
    },
    "db": {
        "region": "us-west-2",
        "tables_config": {
            "discussions": {
                "table_name": "staging.Discussions.v1"
            },
            "participants": {
                "table_name": "staging.Participants.v1"
            },
            "post_bookmarks": {
                "table_name": "staging.PostBookmarks.v1"
            },
            "posts": {
                "table_name": "staging.Posts.v1"
            },
            "users": {
                "table_name": "staging.Users.v1"
            },
            "user_profiles": {
                "table_name": "staging.UserProfiles.v1"
            },
            "viewers": {
                "table_name": "staging.Viewers.v1"
            }
        }
    },
    "twitter": {
        "callback": "http://local.delphishq.com:8080/twitter/callback",
        "redirect": "http://local.delphishq.com:8000"
    },
    "s3_bucket": {
        "media_bucket": "static.chatham.ai",
        "base_key": "media",
        "image_prefix": "images",
        "gif_prefix": "gifs",
        "video_prefix": "videos"
    }
}