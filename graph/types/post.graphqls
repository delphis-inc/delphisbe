type Post implements DiscussionSubscriptionEntity {
    id: ID!
    isDeleted: Boolean!
    deletedReasonCode: PostDeletedReason
    content: String
    discussion: Discussion!
    participant: Participant
    createdAt: String!
    updatedAt: String!
    quotedPost: Post
    mentionedEntities: [Entity!]
    media: Media
    postType: PostType!
}

