type ImportedContent {
    id: ID!
    createdAt: String!
    contentName: String!
    contentType: String!
    link: String!
    overview: String!
    source: String!
    matchingTags: [String!]
}

type ContentQueueRecord {
    discussionID: ID!
    importedContentID: ID!
    createdAt: String!
    updatedAt: String!
    isDeleted: Boolean!
    hasPosted: Boolean!
    matchingTags: [String!]
}